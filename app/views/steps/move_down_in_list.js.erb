<% if @valid %>
    <% if @step_action.type == 'Question' %>
        $("tr#question_<%= @step_action.id %>").each(function () {
            $(this).insertAfter($(this).next("tr.editable-item"));
        });
    <% else %>
        $("tr#instruction_<%= @step_action.id %>").each(function () {
            $(this).insertAfter($(this).next("tr.editable-item"));
        });
    <% end %>
<% end %>